var option=6;

switch(option) {
case 1:
  console.log("You selected the option 1");
  break;
  case 2:
    console.log("You selected the option 2 ");
    break;
    case 3 :
    console.log("You selected the option 3");
    break;
    case 4 :
      console.log("You selected the option 4");
      break;
      case 5 :
        console.log("You selected the option 5");
        break;
        case 6:
          console.log("You selected the option 6");


}
